{
  "name": "chest-idle",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:check": "vite build --mode development && echo 'âœ“ Development build successful'",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "vitest run src/components/__tests__/a11y.spec.ts",
    "test:ui": "vitest --ui",
    "verify": "run-s type-check test lint"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-i18n": "10",
    "vue-router": "^4.5.0",
    "loglevel": "^1.8.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "axe-core": "^4.11.0",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.32.0",
    "jiti": "^2.4.2",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "unocss": "^66.5.6",
    "vite": "^6.2.1",
    "vitest": "^2.1.8",
    "vitest-axe": "^0.1.0",
    "vue-tsc": "^2.2.8",
    "@types/loglevel": "^1.4.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "esbuild": "^0.25.0"
    }
  }
}